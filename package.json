{
  "name": "bun-elysia-modular_auth",
  "author": {
    "name": "Just.Chola",
    "email": "cholasimmons@gmail.com",
    "github": "https://github.com/cholasimmons"
  },
  "version": "0.4.2",
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "esbuild",
    "prisma"
  ],
  "prisma": {
    "seed": "bun tsx prisma/seed.ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bun build --compile --minify --sourcemap --target=bun-linux-x64 src/index.ts --outfile app",
    "compile": "NODE_ENV=production bun build src/index.ts --compile --minify --outfile ./build/bundle",
    "dev": "bun run --watch src/index.ts",
    "prisma:generate": "bunx prisma generate",
    "prisma:seed": "bunx prisma db seed",
    "prisma:deploy": "bunx prisma migrate deploy",
    "start": "NODE_ENV=production bun run src/index.ts"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.7",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/cron": "^1.2.0",
    "@elysiajs/html": "^1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/lucia-auth": "^1.1.0",
    "@elysiajs/static": "^1.2.0",
    "@elysiajs/swagger": "1.0.5",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^6.2.1",
    "arctic": "^1.9.2",
    "bullmq": "^5.34.10",
    "chalk": "^5.4.1",
    "elysia": "^1.2.10",
    "elysia-helmet": "^2.0.0",
    "elysia-htmx": "^1.0.9",
    "elysia-http-status-code": "^1.0.9",
    "elysia-ip": "^1.0.8",
    "elysia-oauth2": "^1.3.0",
    "elysia-rate-limit": "^4.1.0",
    "ioredis": "^5.4.2",
    "logestic": "^1.2.4",
    "lucia": "^3.2.2",
    "mime": "^4.0.6",
    "minio": "^8.0.3",
    "oslo": "^1.2.1",
    "resend": "^3.5.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "bun-types": "^1.1.43",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2"
  },
  "module": "index.js"
}
